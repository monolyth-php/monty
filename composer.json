{
    "name": "monolyth/monty",
    "description": "Monolyth unframework (Full) Monty installer package",
    "keywords": ["PHP","OO","MVC","framework","unframework","front controller"],
    "license": "MIT",
    "authors": [
        {
            "name": "Marijn Ophorst",
            "email": "marijn@monomelodies.nl"
        }
    ],
    "require": {
        "php": ">=7",
        "monolyth/booby": "^1.2.0",
        "monolyth/cesession": "^1.1.0",
        "monolyth/croney": "^0.3.0",
        "monolyth/disclosure": "^2.1.0",
        "monolyth/envy": "^0.5.0",
        "monolyth/formulaic": "^1.6.10",
        "monolyth/frontal": "^1.0.0",
        "monolyth/improse": "^0.7.0",
        "monolyth/improse": "^0.6.1",
        "monolyth/mural": "^0.3.0",
        "monolyth/plumber": "^1.0.0",
        "monolyth/reroute": "^3.5.0"
    },
    "require-dev": {
        "gentry/gentry": "^0.10.0"
    },
    "autoload": {
        "psr-4": {
            "Monolyth\\Monty\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "": "example/src",
            "Monolyth\\Monty\\": "src/",
            "Monolyth\\Monty\\Test\\": "tests/gentry/"
        },
        "files": [
            "tests/HeaderStack.php"
        ]
    },
    "suggests": {
        "twig/twig": "The Twig templating language",
        "ornament/ornament": "Ornament ORM toolkit",
        "quibble/dabble": "Quibble Database Abstraction Layer (and related components)",
        "monomelodies/monki": "Quickly bootstrap an API"
    }
}
